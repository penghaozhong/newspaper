<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="myView"> 

    <attr name="text" format="string" />
    <attr name="icon" format="reference" />
    <attr name="count" format="integer" />
    <attr name="position">
        <enum name="common" value="0" />
        <enum name="top" value="1" />
        <enum name="middle" value="2" />
        <enum name="bottom" value="3" />
    </attr>
    <attr name="item_title" format="string" />
    <attr name="item_desc" format="string" />
    <attr name="item_icon" format="reference" />
    <attr name="item_image" format="reference" />
    <attr name="item_arrow" format="boolean" />
    <attr name="item_check" format="reference">
        <enum name="none" value="-1" />
        <enum name="on" value="1" />
        <enum name="off" value="0" />
    </attr>
    <attr name="title_text" format="string" />
    <attr name="left_text" format="string" />
    <attr name="left_icon" format="reference" />
    <attr name="right_text" format="string" />
    <attr name="right_icon" format="reference" />
    <attr name="right_left_text" format="string" />
    <attr name="right_left_icon" format="reference" />
    <attr name="animationDuration" format="integer" />
    <attr name="panelPosition">
        <enum name="top" value="0" />
        <enum name="bottom" value="1" />
        <enum name="left" value="2" />
        <enum name="right" value="3" />
    </attr>
    <attr name="handle" format="reference" />
    <attr name="content" format="reference" />
    <attr name="linearFlying" format="boolean" />
    <attr name="weight" format="fraction" />
    <attr name="openedHandle" format="reference|color" />
    <attr name="closedHandle" format="reference|color" />
    </declare-styleable>
</resources>
